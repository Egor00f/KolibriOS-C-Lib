617                 \{\par
618                     {\cf19 return} OS::Events::Exit;\par
619                 \}\par
620 \par
621                 {\cf19 for} ({\cf17 const} std::pair<int, Element> &n : _Elements) {\cf20 // \'C7\'E0\'EF\'F3\'F1\'EA \'EE\'E1\'F0\'E0\'E1\'EE\'F2\'F7\'E8\'EA\'EE\'E2 \'E2\'F1\'E5\'F5 \'E8\'F1\'EF\'EE\'EB\'FC\'E7\'F3\'E5\'EC\'FB\'F5 \'FD\'EB\'E5\'EC\'E5\'ED\'F2\'EE\'E2}\par
622                 \{\par
623                     {\cf19 switch} (n.second._type)\par
624                     \{\par
625                     {\cf19 case} Element::Type::Form:\par
626                         n.second.form->ButtonHandler();\par
627                     {\cf19 case} Element::CheckBox:\par
628                         n.second.checkbox->Handler();\par
629                     {\cf19 case} Element::Button:\par
630                         n.second.btn->Handler();\par
631                     \}\par
632                 \}\par
633 \par
634                 {\cf19 break};\par
635             {\cf19 case} OS::Events::Key:\par
636 \par
637                 {\cf19 if} (activeForm != -1)\par
638                 \{\par
639                     _Elements[activeForm].form->Handler(); {\cf20 // \'CE\'E1\'F0\'E0\'E1\'EE\'F2\'F7\'E8\'EA \'E0\'EA\'F2\'E8\'E2\'ED\'EE\'E9 \'ED\'E0 \'F2\'E5\'EA\'F3\'F9\'E8\'E9 \'EC\'EE\'EC\'E5\'ED\'F2 \'F4\'EE\'F0\'EC\'FB}\par
640                 \}\par
641 \par
642                 {\cf19 break};\par
643             \}\par
644             {\cf19 if} (_RealtimeRedraw)\par
645             \{\par
646                 Coord Mouse = mouse::GetMousePositionInWindow();\par
647                 {\cf19 if} (((Mouse.x > 0 && Mouse.y > 0) && Mouse.x < GetWindowSize().x && Mouse.y < GetSkinHeight()) && mouse::GetMouseButtons() == mouse::LeftButton)\par
648                 \{\par
649                     {\cf19 while} (mouse::GetMouseButtons() == mouse::LeftButton)\par
650                     \{\par
651                         Render(mouse::GetMousePositionOnSreen() - Mouse);\par
652                         _ksys_thread_yield();\par
653                     \}\par
654                 \}\par
655             \}\par
656 \par
657             {\cf19 return} event;\par
658         \}\par

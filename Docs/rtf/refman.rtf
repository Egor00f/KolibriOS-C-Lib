534             window::CreateWindow(coord, _size, _title, _colors.frame_area, _TitleColor, _style);\par
535 \par
536             {\cf19 for} ({\cf17 const} std::pair<int, Element> &n : _Elements)\par
537             \{\par
538                 {\cf19 switch} (n.second._type)\par
539                 \{\par
540                 {\cf19 case} Element::Type::TextLabel:\par
541                     n.second.txt->Render();\par
542                     {\cf19 break};\par
543                 {\cf19 case} Element::Type::Button:\par
544                     n.second.btn->Render();\par
545                     {\cf19 break};\par
546                 {\cf19 case} Element::Type::Image:\par

31 {\cf18 void} kolibri_handle_event_redraw(kolibri_window* some_window)\par
32 \{\par
33     {\cf20 /*  Draw windows with system color table. */}\par
34 \par
35     _ksys_start_draw();\par
36 \par
37     _ksys_create_window(\par
38         some_window->topleftx,\par
39         some_window->toplefty,\par
40         some_window->sizex,\par
41         some_window->sizey,\par
42         some_window->window_title,\par
43         KolibriLib::OS::GetSystemColors().work_area,\par
44         some_window->XY\par
45     );\par
46 \par
47     {\cf20 /* Enumerate and draw all window elements here */}\par
48     {\cf19 if}(some_window->elements) {\cf20 /* Draw all elements added to window */}\par
49     \{\par
50         kolibri_window_element* current_element = some_window -> elements;\par
51 \par
52         {\cf19 do}\par
53         \{\par
54             {\cf20 /* The redraw_fn serves as draw_fn on initial draw */}\par
55             {\cf19 if}(({\cf18 int})kolibri_gui_op_table[current_element -> type].redraw_fn > 0)  {\cf20 // -1 if DLL link fail}\par
56                 kolibri_gui_op_table[current_element -> type].redraw_fn(current_element -> element);\par
57 \par
58 {\cf20 //sie after fixing calling conventions no more needed}\par
59 {\cf20 /*}\par
60 {\cf20             switch(current_element -> type)}\par
61 {\cf20             \{}\par
62 {\cf20                 case KOLIBRI_EDIT_BOX:}\par
63 {\cf20                 case KOLIBRI_CHECK_BOX:}\par
64 {\cf20                     __asm__ volatile("push $0x13371337"::); / * Random value pushed to balance stack * /}\par
65 {\cf20                                   / * otherwise edit_box_draw leaves stack unbalanced * /}\par
66 {\cf20                                   / * and GCC jumps like a crazy motha' fucka' * /}\par
67 {\cf20 }\par
68 {\cf20                     break;}\par
69 {\cf20                 default:}\par
70 {\cf20                     break;}\par
71 {\cf20             \}}\par
72 {\cf20 */}\par
73             current_element =(kolibri_window_element *) current_element -> next;\par
74 \par
75         \} {\cf19 while}(current_element != some_window->elements); {\cf20 /* Have we covered all elements? */}\par
76     \}\par
77 \}\par
78 \par
79 {\cf18 void} kolibri_handle_event_key(kolibri_window* some_window, ksys_oskey_t key)\par
80 \{\par
81     {\cf20 /* Enumerate and trigger key handling functions of window elements here */}\par
82     {\cf19 if}(some_window->elements)\par
83     \{\par
84         kolibri_window_element *current_element = some_window -> elements;\par
85 \par
86         {\cf19 do}\par
87         \{\par
88             {\cf20 /* Only execute if the function pointer isn't NULL, or -1 (fail to find in export table) */}\par
89             {\cf19 if}(({\cf18 int})kolibri_gui_op_table[current_element -> type].key_fn > 0)\par
90                 kolibri_gui_op_table[current_element -> type].key_fn(current_element -> element, key);\par
91 \par
92             current_element = (kolibri_window_element *)current_element->next;\par
93         \} {\cf19 while}(current_element != some_window->elements); {\cf20 /* Have we covered all elements? */}\par
94     \}\par
95 \}\par
96 \par
97 {\cf18 void} kolibri_handle_event_mouse(kolibri_window* some_window)\par
98 \{\par
99   {\cf20 /* Enumerate and trigger mouse handling functions of window elements here */}\par
100   {\cf19 if}(some_window->elements)\par
101     \{\par
102       kolibri_window_element *current_element = some_window -> elements;\par
103 \par
104       {\cf19 do}\par
105     \{\par
106       {\cf19 if}(({\cf18 int})kolibri_gui_op_table[current_element -> type].mouse_fn > 0)\par
107         kolibri_gui_op_table[current_element -> type].mouse_fn(current_element -> element);\par
108 \par
109       current_element =(kolibri_window_element *) current_element->next;\par
110 \par
111     \} {\cf19 while}(current_element != some_window->elements); {\cf20 /* Have we covered all elements? */}\par
112     \}\par
113 \}\par

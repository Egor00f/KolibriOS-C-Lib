62 \};\par
63 \par
64 {\cf20 /* Initializes an Editbox with sane settings, sufficient for most use.}\par
65 {\cf20    This will let you create a box and position it somewhere on the screen.}\par
66 {\cf20    The text_buffer is a pointer to a character array and needs to be as long as}\par
67 {\cf20    AT LEAST MAX_CHARS + 1.If the text_buffer is smaller, it will crash if user}\par
68 {\cf20    types more characters than what will fit into the text buffer.}\par
69 {\cf20 }\par
70 {\cf20    Allocating buffer space automatically so that programmer can be carefree now.}\par
71 {\cf20    This also automatically adjusts the size of edit box so that it can hold enough characters.}\par
72 {\cf20 }\par
73 {\cf20    All you need is :}\par
74 {\cf20 }\par
75 {\cf20    tlx,tly = Coordinates of the beginning of the edit box.}\par
76 {\cf20    max_chars = Limit of number of characters user can enter into edit box.}\par
77 {\cf20 */}\par
78 \par
79 edit_box* kolibri_new_edit_box({\cf18 unsigned} {\cf18 int} tlx, {\cf18 unsigned} {\cf18 int} tly, {\cf18 unsigned} {\cf18 int} max_chars, {\cf18 void} *editbox_interlock)\par
80 \{\par
81     {\cf18 unsigned} {\cf18 int} PIXELS_PER_CHAR = 7;\par
82     edit_box *new_textbox = {\cf17 new} edit_box;\par
83     {\cf18 char} *text_buffer = ({\cf18 char} *)calloc(max_chars + 2, {\cf17 sizeof}({\cf18 char})); {\cf20 // +2 as asked in box_lib src}\par
84 \par
85     {\cf20 /* Update blur_border_color and shift_color from box_lib.mac macro */}\par
86     {\cf20 /* edit_boxes_set_sys_color */}\par
87 \par
88     new_textbox -> width                = max_chars * PIXELS_PER_CHAR;\par
89     new_textbox -> left                 = tlx;\par
90     new_textbox -> top                  = tly;\par
91     new_textbox -> color                = 0xFFFFFF; {\cf20 /* Always make white edit boxes */}\par
92     new_textbox -> shift_color          = 0x6a9480;\par
93     new_textbox -> focus_border_color   = KolibriLib::OS::GetSystemColors().work_graph;\par
94     new_textbox -> blur_border_color    = 0x6a9480;\par
95     new_textbox->text_color             = KolibriLib::OS::GetSystemColors().work_text; {\cf20 /* Always black text when typing */}\par
96     new_textbox -> max                  = max_chars;\par
97     new_textbox -> text                 = text_buffer;\par
98     new_textbox -> mouse_variable       = editbox_interlock;\par
99     new_textbox -> flags                = 0x00000000;\par
100 \par
101     {\cf19 return} new_textbox;\par
102 \}\par
103 \par

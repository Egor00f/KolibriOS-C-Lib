188 {\cf18 void} kolibri_window_add_element(kolibri_window *some_window, {\cf17 enum} KOLIBRI_GUI_ELEMENT_TYPE element_type, {\cf18 void} *some_gui_element)\par
189 \{\par
190     kolibri_window_element *new_element = (kolibri_window_element *)malloc({\cf17 sizeof}(kolibri_window_element));\par
191 \par
192     new_element -> type = element_type;\par
193     new_element -> element = some_gui_element;\par
194 \par
195     {\cf19 if}(!(some_window->elements)) {\cf20 /* No elements in window yet */}\par
196         \{\par
197             some_window->elements = new_element;\par
198             some_window->elements -> prev = some_window->elements;\par
199             some_window->elements -> next = some_window->elements;\par
200         \}\par
201     {\cf19 else}\par
202         \{\par
203             kolibri_window_element *last_element = (kolibri_window_element*)some_window->elements->prev;\par
204 \par
205             last_element -> next = new_element;\par
206             new_element -> next = some_window -> elements; {\cf20 /* start of linked list  */}\par
207             some_window -> elements -> prev = new_element;\par
208             new_element -> prev = last_element;\par
209         \}\par
210 \}\par

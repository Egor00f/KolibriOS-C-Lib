156     {\cf18 int} filesize = ftell(f);\par
157     rewind(f);\par
158     {\cf18 char} *fdata = ({\cf18 char} *)malloc(filesize);\par
159 \par
160     {\cf19 if} (!fdata)\par
161     \{\par
162         {\cf17 const} {\cf18 char} a[] = {\cf22 "No memory for file: "};\par
163         {\cf18 char} *b;\par
164         strcat(b, a);\par
165         strcat(b, Path);\par
166         strcat(b, {\cf22 "\\n"});\par
167         strcat(b, {\cf22 "malloc not return ptr\\n"});\par
168         _ksys_debug_puts(b);\par
169     \}\par
170 \par
171     img_size = (int32_t)fread(fdata, 1, filesize, f);\par
172 \par
173     {\cf19 if} (ferror(f))\par
174     \{\par
175         {\cf17 const} {\cf18 char} *a = {\cf22 "Error reading file "};\par
176         {\cf18 char} *b;\par
177         strcat(b, a);\par
178         strcat(b, Path);\par
179         strcat(b, {\cf22 " \\n "});\par
180         _ksys_debug_puts(b);\par
181     \}\par
182 \par
183     fclose(f);\par
184 \par
185     Image_t *buff = img_decode(({\cf18 void} *)fdata, img_size, 0);\par
186 \par
187     free(fdata);\par
188 \par
189     {\cf19 return} buff;\par
190 \}\par
